<?xml version="1.0" encoding="UTF-8"?>
<deployment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:jboss:bean-deployer:2.0 bean-deployer_2_0.xsd"
	xmlns="urn:jboss:bean-deployer:2.0">
     
    <bean name="Trunk" class="org.mobicents.media.server.impl.resource.zap.Trunk">
        <property name="name">/mobicents/media/DS0</property>
        <property name="span">1</property>
        <property name="firstCIC">1</property>
        <property name="channels">1-10</property>
        <property name="connectionFactory"><inject bean="default-connection" /></property>
    </bean>
    
</deployment>